{
  "name": "@package/moment",
  "version": "0.0.1",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "types",
  "scripts": {
    "build:types": "tsc",
    "build": "cross-env NODE_ENV=production babel ./src --out-dir ./lib --extensions '.ts,.tsx,.js,.jsx' --copy-files --no-copy-ignored",
    "start:dev": "cross-env NODE_ENV=development babel ./src --out-dir ./lib --extensions '.ts,.tsx' --copy-files --watch"
  },
  "dependencies": {
    "moment": "2.29.4",
    "moment-timezone": "0.5.34"
  },
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-typescript": "7.18.6",
    "@types/moment": "2.13.0",
    "cross-env": "7.0.3"
  }
}
